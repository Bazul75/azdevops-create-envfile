{
    "manifestVersion": 1,
    "id": "create-envfile",
    "name": "Generate .env File",
    "version": "1.0.6",
    "publisher": "Bazul",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Task to generate a .env file based on a Variable Group",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon.png"
    },
    "files": [
        {
            "path": "dist"
        }
    ],
    "contributions": [
        {
            "id": "generate-env-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "dist"
            }
        }
    ]
}